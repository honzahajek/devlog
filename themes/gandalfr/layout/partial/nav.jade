ul.nav.nav-list
    each value, key in theme.menu
        - var re = /^(http|https):\/\/*/gi;
        - var tar = re.test(value) ? "_blank" : "_self"
        - var act = !re.test(value) && page.path === value.substr(1)+'index.html'
        li.nav-list-item(class={active: act})
            a.nav-list-link(href=url_for(value) target=tar)
                != key.toUpperCase()
